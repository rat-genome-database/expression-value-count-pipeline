<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
    <bean id="main" class="edu.mcw.rgd.expressionValueCount.Main">
        <property name="version" value="Calculate Gene Expression Value Counts -- build 10/04/2024"/>
        <property name="species">
            <map>
                <entry key="1" value="Human"/>
